<script>

    var randArray = [[1,3,5,7],
        [2,4,6,8],
        [9,11,13],
        [10,12,14]];

    sum(randArray);

    function sum(numbers) {
        var total = 0;

        for (var i = 0; i < numbers.length; i++) {
            for (var j = 0; j < numbers[i].length; j++) {
                total += numbers[i][j];
            }
        }
        console.log(total);
    }


    debugger;



</script>


